<manifest xmlns:android="http://schemas.android.com/apk/res/android" package="com.example.bossintent" android:versionCode="1" android:versionName="1.0">
<uses-sdk android:minSdkVersion="8" android:targetSdkVersion="16"/>
<application android:allowBackup="true" android:icon="@drawable/ic_launcher" android:label="@string/app_name" android:theme="@style/AppTheme">
<activity android:name="com.example.bossintent.MainActivity" android:label="Explicit Intent Exercise">
<intent-filter>
<action android:name="android.intent.action.MAIN"/>
<category android:name="android.intent.category.LAUNCHER"/>
</intent-filter>
</activity>
<activity android:name=".MyStore" android:label="Store Management"></activity>
<activity android:name=".MyBoss" android:label="Boss Side"></activity>
</application>
</manifest>